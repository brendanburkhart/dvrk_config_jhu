/* -*- Mode: Javascript; tab-width: 4; indent-tabs-mode: nil; c-basic-offset: 4 -*- */
{
    "io": {
        // "port": "udp"
        // "port": "fw"
    }
    ,
    "component-manager": {
        "components":
        [
            {
                "shared-library": "sawForceDimensionSDK",
                "class-name": "mtsForceDimension",
                "constructor-arg": {
                    "Name": "ForceDimensionSDK"
                },
                "configure-parameter": "omega7-11311.json"
            }
            ,
            {
                "shared-library": "sawJoystick",
                "class-name": "mtsJoystick",
                "constructor-arg": {
                    "Name": "joystick"
                },
                "configure-parameter": "misc/sawJoystickConfiguration.json"
            }
        ]
    }
    ,
    "console-inputs":
    {
        "operator-present": {
            "component": "joystick",
            "interface": "OperatorPresent"
        }
        ,
        "clutch": {
            "component": "joystick",
            "interface": "Clutch"
        }
    }
    ,
    "arms":
    [
        {
            "name": "MTMR",  // created previously using custom components
            "type": "MTM_GENERIC",
            "component": "ForceDimensionSDK",
            "interface": "MTMR"
        }
        ,
        {
            "name": "PSM1",
            "type": "PSM_Si",
            "serial": "334809"
            // ,
            // "socket-server": true,
            // "remote-ip": "127.0.0.1",
            // "port": 10001
        }
    ]
    ,
    "psm-teleops":
    [
        {
            "mtm": "MTMR",
            "psm": "PSM1",
            "configure-parameter": {
                "gripper-scaling": {
                    "max": 0.492,
                    "zero": 0.05
                },
                "align-mtm": false,
                "rotation" : [[ 0.0000, -1.0000,  0.0000],
                              [ 1.0000,  0.0000,  0.0000],
                              [ 0.0000,  0.0000,  1.0000]]
            }
        }
    ]
}
